#include <stdio.h>
int main () {
    int a,b,c;
    scanf("%d" ,&a);
    scanf("%d" ,&b);
    scanf("%d" ,&c);
    int result=(a+b+c)*7;
    if(result%2==0){
        printf("Even");
    }
    else{
        printf("Odd");
    }
}

/*
Tool output
/tmp/HohVPDvRRl.o
4
6
8
Even

=== Code Execution Successful ===
